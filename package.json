{
  "name": "tabris-markdown",
  "version": "0.1.0",
  "description": "Tabris.js markdown parser",
  "author": "Elmi Ahmadov <eahmadov@eclipsesource.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c ./rollup.js && npm run dts",
    "clean": "rm -rf ./dist ./demo-dist",
    "dts": "dts-generator --project ./tsconfig.json --prefix tabris-markdown --name tabris-markdown --main tabris-markdown/index --out ./dist/index.d.ts",
    "test": "npm run mocha && npm run build",
    "mocha": "ts-mocha -p ./test/tsconfig.json ./test/**/*.spec.ts",
    "start": "tabris serve -w -a -m ./demo-dist/demo/index.js",
    "watch": "tsc -w -p ./tsconfig-demo.json --preserveWatchOutput --inlineSourceMap"
  },
  "peerDependencies": {
    "tabris": "^3.7.1",
    "tabris-decorators": "^3.7.1",
    "marked": "^1.2.9"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/marked": "^1.2.2",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^9.0.10",
    "chai": "^4.3.0",
    "dts-generator": "^2.1.0",
    "marked": "^2.0.0",
    "mocha": "^8.2.1",
    "rollup": "~2.26.0",
    "rollup-plugin-typescript2": "~0.27.0",
    "sinon": "^9.2.4",
    "tabris": "^3.7.1",
    "tabris-decorators": "^3.7.1",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.1.3"
  }
}
